    "rimraf": "^3.0.2",
    "ts-loader": "^9.3.1",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "@protobuf-ts/plugin": "^2.9.4",
    "circular-json": "^0.5.9",
    "google-protobuf": "^3.21.4",
    "pbf": "^4.0.1",
